{
  "name": "@tdm-design/icons",
  "version": "0.0.21",
  "description": "TDM Design Icon Library - React icon components with tree-shaking support",
  "contributes": [
    "hongnv1107<hong.nv1107@gmail.com>"
  ],
  "sideEffects": false,
  "main": "./lib/index.js",
  "unpkg": "./dist/index.umd.js",
  "module": "./es/index.js",
  "typings": "./lib/index.d.ts",
  "files": [
    "dist",
    "es",
    "lib",
    "*.js",
    "*.d.ts",
    "README.md"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hongnv1107/tdm-design-icons.git"
  },
  "scripts": {
    "clean": "node scripts/cleanup.cjs",
    "generate": "rimraf src/icons && cross-env TS_NODE_PROJECT=scripts/tsconfig.json node -r ts-node/register scripts/generate.ts --target=icon",
    "compile": "father build",
    "postcompile": "npm run clean && cross-env TS_NODE_PROJECT=scripts/tsconfig.json node -r ts-node/register scripts/generate.ts --target=entry",
    "lint": "eslint src/ --ext .tsx,.ts",
    "prepublishOnly": "npm run generate && npm run compile",
    "test:usage": "npm run generate && npm run compile && node scripts/test-usage.cjs",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "npm run generate && npm run compile && changeset publish",
    "demo:dev": "vite",
    "demo:build": "vite build",
    "demo:preview": "vite preview"
  },
  "keywords": [
    "react",
    "icons",
    "svg",
    "typescript",
    "component-library",
    "tree-shaking"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.8",
    "@rc-component/father-plugin": "^2.1.3",
    "@types/node": "^25.0.3",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "cross-env": "^7.0.3",
    "father": "^4.6.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0",
    "vite": "^6.0.0"
  },
  "dependencies": {
    "@rc-component/util": "^1.0.0",
    "clsx": "^2.1.1"
  }
}
